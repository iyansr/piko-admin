{
  "kind": "collectionType",
  "collectionName": "places",
  "info": {
    "singularName": "place",
    "pluralName": "places",
    "displayName": "Place",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "string",
      "required": false
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "rating": {
      "type": "decimal"
    },
    "menus": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::menu.menu",
      "mappedBy": "place"
    },
    "slug": {
      "type": "uid",
      "targetField": "name"
    },
    "lat": {
      "type": "string",
      "required": true
    },
    "lng": {
      "type": "string",
      "required": true
    },
    "gallery": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": true
    }
  }
}
